public class Main{
    public static void main(String args[]){
        int array[]={1,2,3,4,5};
        int f=3,b=0;
        for(int i=0;i<array.length;i++){
            if(f==array[i]){
                b = 1;
                System.out.println("found");
                break;
            }
    
        }
        if(b==0){
            System.out.println("not found");
        }
    }
}
